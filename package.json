{
  "name": "slack-channel-list-to-spreadsheet",
  "version": "1.0.1",
  "description": "export slack channel to google sheet",
  "main": "./src/index.js",
  "scripts": {
    "start": "node ./src/index.js",
    "commit": "git cz",
    "standard": "semistandard --fix",
    "document": "documentation build -f md ./src/ | add-text-to-markdown ./DOCUMENT.md --section 'DocumentationJS' --write",
    "readme": "npx readme-md-generator",
    "test": "mocha"
  },
  "author": "code4kit",
  "contributors": [
    "chensokheng",
    "waricoma"
  ],
  "license": "MIT",
  "homepage": "https://github.com/code4kit/slack-channel-list-to-spreadsheet#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code4kit/slack-channel-list-to-spreadsheet.git"
  },
  "bugs": {
    "url": "https://github.com/code4kit/slack-channel-list-to-spreadsheet/issues"
  },
  "engines": {
    "yarn": ">=1.19.1",
    "node": ">=12.11.1"
  },
  "dependencies": {
    "@slack/web-api": "^5.0.1",
    "arr-to-sheet-client": "^1.0.1",
    "cron": "^1.7.1",
    "dotenv": "^10.0.0",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "add-text-to-markdown": "2.0.0",
    "commitizen": "4.0.3",
    "cz-emoji": "1.2.0",
    "documentation": "12.1.4",
    "mocha": "7.0.0",
    "readme-md-generator": "1.0.0",
    "semistandard": "14.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji"
    }
  },
  "semistandard": {
    "global": [
      "describe",
      "it"
    ]
  }
}
